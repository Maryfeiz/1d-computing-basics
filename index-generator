#!/usr/bin/env bash

# add the first section of html (static section)
cat << EOF > index.html

<!doctype html>
<html>
  <head>
    <title>Images</title>
  </head>
  <body>
   <center>
    <h1>MATH COLLECTION</h1>
   </center>

  <table border="5" bordercolor="red" align="center">
EOF

# add the second section of html (dynamic section)


# add the last section of html (static section)

cat << EOF >> index.html
  </table>
  </body>
</html>
EOF
